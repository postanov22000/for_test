(function(Drupal){'use strict';Drupal.behaviors.ajaxDropdownLanguage={attach:function(context){const observer=new MutationObserver(function(mutations){mutations.forEach(function(mutation){for(let i=0;i<mutation.addedNodes.length;i++)if(mutation.addedNodes[i].id==='block-dropdownlanguagecontent')Drupal.attachBehaviors();});});const target=context.querySelector('.main-header__language-switcher');if(target)observer.observe(target,{childList:true,subtree:true});}};}(Drupal));;
